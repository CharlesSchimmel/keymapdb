---
permalink: "/index.json"
---
[
    {% for post in collections.posts %}
    {
        "url": "{{ post.url | url }}",
        "title": "{{ post.data.title }}",
        "date": "{{ post.data.page.date | readableDate }}",
        "stagger": "{{post.data.stagger}}"
    }{% if not loop.last %},{% endif %}
    {% endfor %}
]
